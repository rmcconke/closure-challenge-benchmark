FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    location    "2000";
    object      bijDelta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0 0 0 0);

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform (1.20814e-37 0 0 0 0 0);
    }
    bottom
    {
        type            calculated;
        value           uniform (1.20814e-37 0 0 0 0 0);
    }
    sides
    {
        type            empty;
    }
    outlet
    {
        type            calculated;
        value           uniform (1.20814e-37 0 0 0 0 0);
    }
    top
    {
        type            symmetry;
    }
}


